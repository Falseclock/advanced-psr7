{
  "name": "falseclock/advanced-psr7",
  "description": "Advanced implementation of PSR7",
  "type": "library",
  "license": "MIT",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "homepage": "https://github.com/Falseclock/advanced-psr7",
  "authors": [
    {
      "name": "Nurlan Mukhanov",
      "email": "nurike@gmail.com",
      "homepage": "https://github.com/Falseclock/",
      "role": "Author"
    }
  ],
  "repositories": [
    {
      "name": "falseclock/common-library",
      "type": "path",
      "url": "../common-library",
      "options": {
        "symlink": false
      }
    }
  ],
  "require": {
    "php": ">=8.0",
    "falseclock/common-library": "*",
    "guzzlehttp/psr7": "^2.4.1",
    "ext-intl": "*",
    "ext-openssl": "*",
    "ext-bcmath": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.25",
    "php-coveralls/php-coveralls": "^2.5.3"
  },
  "autoload": {
    "psr-4": {
      "Falseclock\\AdvancedPSR7\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Falseclock\\AdvancedPSR7\\Tests\\": "tests/"
    }
  }
}
